# Maritaca AI API Key (required)
MARITACA_API_KEY=your_maritaca_api_key_here

# OpenAI API Key (required for moderation)
OPENAI_API_KEY=your_openai_api_key_here

# Maritaca Base URL (optional, defaults to https://chat.maritaca.ai/api)
# Use this to override the base URL for different environments (e.g., staging, production)
MARITACA_BASE_URL=https://chat.maritaca.ai/api

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/briefing_enhancer

# Logging
LOG_LEVEL=INFO

# LangSmith Tracing (optional)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=
LANGSMITH_PROJECT=Briefing Enhancer API
